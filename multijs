# URL pattern to extract endpoints from JS files
url_pattern = r'(https?:\/\/[a-zA-Z0-9\-._~:/?#\[\]@!$&\'()*+,;=%]+)'

# Function to crawl subdomain with katana and fetch URLs
def crawl_subdomain(subdomain):
    try:
        result = subprocess.run(['katana', '-u', f'http://{subdomain}', '-silent'], stdout=subprocess.PIPE, stderr=subprocess.PIPE)
        return result.stdout.decode('utf-8').splitlines()
    except Exception as e:
        print(f"Error fetching URLs from {subdomain}: {e}")
        return []

# Function to extract JS files from URLs
def filter_js_files(urls):
    return {url for url in urls if url.endswith(".js")}

# Function to fetch JS file from web.archive.org if not directly accessible
def fetch_from_webarchive(js_url):
    archive_url = f"https://web.archive.org/web/*/{js_url}"
    try:
        response = requests.get(archive_url, timeout=5)
        if response.status_code == 200:
            return response.text
        return None
    except Exception as e:
        print(f"Error fetching {js_url} from Web Archive: {e}")
        return None

# Function to scan JavaScript files for secrets & URLs
def scan_js_for_secrets_and_urls(js_urls):
    found_secrets = {}
    found_urls = {}

    for js_url in js_urls:
        try:
            response = requests.get(js_url, timeout=5)
            if response.status_code == 200:
                js_content = response.text
            else:
                print(f"Error accessing {js_url}. Trying web archive...")
                js_content = fetch_from_webarchive(js_url)

            if js_content:
                # Extract secrets
                secrets = {}
                for secret_name, pattern in secret_patterns.items():
                    matches = re.findall(pattern, js_content)
                    if matches:
                        secrets[secret_name] = matches

                if secrets:
                    found_secrets[js_url] = secrets

                # Extract URLs
                urls = re.findall(url_pattern, js_content)
                if urls:
                    found_urls[js_url] = list(set(urls))  # Remove duplicates

        except Exception as e:
            print(f"Error scanning {js_url}: {e}")

    return found_secrets, found_urls

# Function to save results to a uniquely named JSON file
def save_results_to_json(subdomains, js_files, secrets, js_urls, filename_prefix="results"):
    # Generate a unique filename based on current timestamp
    timestamp = time.strftime("%Y%m%d_%H%M%S")
    filename = f"{filename_prefix}_{timestamp}.json"
    
    results = {
        "subdomains": subdomains,
        "javascript_files": list(js_files),
        "secrets_found": secrets,
        "urls_in_js": js_urls,
    }

    with open(filename, "w") as json_file:
        json.dump(results, json_file, indent=4)

    print(f"\nResults saved to {filename}")

# Function to run subfinder if a domain is provided
def run_subfinder_for_domain(domain):
    try:
        result = subprocess.run(['subfinder', '-d', domain, '-silent'], stdout=subprocess.PIPE, stderr=subprocess.PIPE)
        subdomains = result.stdout.decode('utf-8').splitlines()
        return subdomains
    except Exception as e:
        print(f"Error running subfinder for domain {domain}: {e}")
        return []

# Main function
def main(input_data):
    all_js_files = set()

    # Check if input_data is a domain or a file
    if '.' in input_data:  # Domain provided
        print(f"Scanning domain: {input_data}")
        subdomains = run_subfinder_for_domain(input_data)

        if not subdomains:
            print(f"No subdomains found for domain {input_data}. Exiting.")
            return

        print(f"Found {len(subdomains)} subdomains.")
    else:  # File containing subdomains
        print(f"Reading subdomains from file: {input_data}")
        try:
            with open(input_data, 'r') as file:
                subdomains = [line.strip() for line in file.readlines()]
        except Exception as e:
            print(f"Error reading subdomain file: {e}")
            return

        if not subdomains:
            print("No subdomains found in the provided file.")
            return

    # Process each subdomain
    for subdomain in subdomains:
        print(f"Crawling subdomain: {subdomain}")
        urls = crawl_subdomain(subdomain)

        js_files = filter_js_files(urls)
        all_js_files.update(js_files)

    print(f"Scanning {len(all_js_files)} JavaScript files for secrets & URLs...")
    secrets, extracted_urls = scan_js_for_secrets_and_urls(all_js_files)

    # Display found secrets
    if secrets:
        print("\nFound Secrets:")
        for js_file, secret_data in secrets.items():
            print(f"\n[+] {js_file}")
            for secret_name, values in secret_data.items():
                for value in values:
                    print(f"    - {secret_name}: {value}")
    else:
        print("\nNo secrets found.")

    # Display extracted URLs
    if extracted_urls:
        print("\nExtracted URLs from JavaScript files:")
        for js_file, urls in extracted_urls.items():
            print(f"\n[+] {js_file}")
            for url in urls:
                print(f"    - {url}")
    else:
        print("\nNo URLs found in JavaScript files.")

    # Save results to uniquely named JSON file
    save_results_to_json(subdomains, all_js_files, secrets, extracted_urls)

# Entry point for script execution
if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python3 main.py <domain_or_subdomain_file>")
        sys.exit(1)

    input_data = sys.argv[1]  # Domain or file containing subdomains

    print(f"Starting scan with input: {input_data}")
    main(input_data)
